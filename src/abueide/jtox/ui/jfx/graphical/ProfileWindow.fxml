<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<AnchorPane xmlns="http://javafx.com/javafx/8.0.76-ea" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox prefWidth="205.0" spacing="10.0">
            <children>
                <TextField fx:id="profileName" promptText="Profile Name"/>
                <PasswordField fx:id="encryptionKey" promptText="Encryption Key (Optional)"/>
                <HBox alignment="CENTER" spacing="10.0">
                    <children>
                        <Button fx:id="cancelButton" mnemonicParsing="false" text="Cancel"/>
                        <Button fx:id="saveButton" mnemonicParsing="false" text="Save"/>
                    </children>
                </HBox>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </VBox>
    </children>
</AnchorPane>
