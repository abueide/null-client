<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="218.0" xmlns="http://javafx.com/javafx/8.0.76-ea" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <HBox layoutX="117.0" layoutY="143.0" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <VBox alignment="BOTTOM_LEFT" spacing="10.0" HBox.hgrow="ALWAYS">
                    <children>
                        <ListView fx:id="profileListView" VBox.vgrow="ALWAYS"/>
                        <CheckBox mnemonicParsing="false" text="Load profile(s) on startup"/>
                    </children>
                </VBox>
                <VBox spacing="10.0">
                    <children>
                        <Button fx:id="createProfileButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="25.0"/>
                        <Button fx:id="deleteProfileButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="25.0"/>
                        <Button fx:id="editProfileButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="25.0"/>
                        <Button fx:id="settingsButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="25.0"/>
                        <Button fx:id="loadProfileButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="25.0"/>
                    </children>
                </VBox>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </HBox>
    </children>
</AnchorPane>
